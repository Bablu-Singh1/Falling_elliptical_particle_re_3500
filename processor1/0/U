FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            fixedValue;
        value           nonuniform List<vector> 
0
;
    }
    right
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    top
    {
        type            fixedValue;
        value           nonuniform List<vector> 
0
;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


